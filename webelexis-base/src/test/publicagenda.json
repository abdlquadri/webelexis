  "mock-sql": {
    "task": "reply",
    "listen-address": "ch.webelexis.sql",
    "match": [{
        "message": {
          "action": "prepared",
          "statement": "SELECT Tag.+",
          "values": [
            "[0-9]{8,8}",
            "[0-9]{8,8}",
            "\\w+"
          ]
        },

        "case-sensitive": true,
        "match-reply": {
          "status": "ok",
          "results": [
            [
              "20150331",
              "700",
              "30",
              "urps",
              "apatient"
            ],

            [
              "20150331",
              "730",
              "60",
