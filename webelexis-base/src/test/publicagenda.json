              "anotherPatient"
            ],

            [
              "20150331",
              "830",
              "30",
              "unfree",
              "123456"
            ]
          ]
        }
      },

      {
        "message": {
          "action": "prepared",
          "statement": "INSERT INTO AGNTERMINE.+",
          "values": [
            ".+",
            "[0-9]+",
            "[0-9]{8,8}",
            ".+",
            "[0-9]+",
            "[0-9]+",
            ".+",
            ".+",
            ".+",
            ".+"
          ]
        },

        "case-sensitive": true,
        "match-reply": {
          "status": "ok"
        }
      },

      {
        "message": {
          "action": "prepared",
          "statement": "UPDATE AGNTERMINE.+",
          "values": [
            "1",
            ".+",
            "[0-9]{8,8}",
            "[0-9]+"
          ]
        },

        "case-sensitive": true,
        "match-reply": {
          "status": "ok"
        }
      }
    ],

    "nomatch-reply": {
      "status": "no matching message found (mock-sql)"
    }
  },

